{
  "InputRootFolder": "/input/",
  "InputFolderRegex": "/input/(?<Person>[A-Za-z0-9-_]+)",
  "FileNameRegex": "\"(?<Filename>.*)\\.(?<Fileextension>.*)", // capture the file name
  "ProcessFile": "(?<Filename>.*)\\.(?<Fileextension>heic)", // convert file if match the file. capture the word from regex
  "CopyFile": "(?<Filename>.*)\\.(?<Fileextension>mov)", // copy file if match the file. capture the word from regex
  "OutputFolder": "/output/{Person}/{FolderDate:yyyyMMdd}",
  "OutputFile": "{Filename}.{Fileextension}"
} 